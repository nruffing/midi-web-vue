{
  "name": "midi-web-vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "format": "npx prettier --write \"./**/*.{ts,json,vue,css}\"",
    "upgrade-all": "ncu --upgrade",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/uuid": "^9.0.5",
    "data-grid-vue": "^1.0.0",
    "debounce": "^1.2.1",
    "pinia": "^2.1.6",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/debounce": "^1.2.2",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-check-updates": "^16.14.6",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.11"
  },
  "lint-staged": {
    "*.{ts,json,vue,css}": "prettier --write"
  }
